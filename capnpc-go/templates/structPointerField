func (s {{.Node.Name}}) {{.Field.Name|title}}() ({{.G.Capnp}}.Ptr, error) {
	{{if .Default.IsValid -}}
	p, err := s.Struct.Ptr({{.Field.Slot.Offset}})
	if err != nil {
		return nil, err
	}
	return p.Default({{.Default}})
	{{- else -}}
	return s.Struct.Ptr({{.Field.Slot.Offset}})
	{{- end}}
}

